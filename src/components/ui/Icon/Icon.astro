---
import { IconComponent } from './Icon.component';

interface Props {
	name: string;
	size?: number;
	color?: string;
	stroke?: number;
	className?: string;
	style?: React.CSSProperties;
}

const { name, size, color, stroke, className, style } = Astro.props;
---

<IconComponent
	{name}
	{size}
	{color}
	{stroke}
	{className}
	{style}
	client:load
/>
